{
  "name": "@ambertide/generate-realm",
  "version": "0.2.2",
  "description": "Generate realm files from JSON",
  "author": "Ege Ã–zkan @ambertide",
  "bin": "./bin/run",
  "homepage": "https://github.com/ambertide/generate-realm",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "ambertide/generate-realm",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.3",
    "realm": "^11.5.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.10",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.16",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typedoc": "^0.23.28",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "generate-realm",
    "dirname": "generate-realm",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "generate-docs": "typedoc"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/ambertide/generate-realm/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
